{
  "compilerOptions": {
    "target": "esnext",
    "module": "umd",
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true // 隐藏一些报错
  },
  "include": ["./**/*"],
  "exclude": ["./dist/**/*"]
}
